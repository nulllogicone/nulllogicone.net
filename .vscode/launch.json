{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch NulllogiconeApi",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-nulllogicone-api",
            "program": "${workspaceFolder}/NulllogiconeApi/NulllogiconeApi/bin/Debug/net10.0/NulllogiconeApi.dll",
            "args": [],
            "cwd": "${workspaceFolder}/NulllogiconeApi/NulllogiconeApi",
            "console": "internalConsole",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": "Launch NulllogiconeApi (Docker)",
            "type": "docker",
            "request": "launch",
            "preLaunchTask": "docker-run: debug",
            "netCore": {
                "appProject": "${workspaceFolder}/NulllogiconeApi/NulllogiconeApi/NulllogiconeApi.csproj"
            }
        }
    ]
}