meta {
  name: SPARQL Count Entities
  type: http
  seq: 21
}

post {
  url: {{host}}/sparql
  body: text
  auth: inherit
}

headers {
  Content-Type: application/sparql-query
  Accept: text/csv
}

body:text {
  PREFIX nlo: <http://nulllogicone.net/schema.rdfs#> 
  
  SELECT ?type (COUNT(*) as ?count) 
  WHERE { ?entity a ?type . } 
  GROUP BY ?type ORDER BY DESC(?count)
}

settings {
  encodeUrl: true
  timeout: 0
}
