meta {
  name: Stamm/<guid> rdf
  type: http
  seq: 6
}

get {
  url: {{host}}/stamm/{{StammGuid}}.rdf
  body: none
  auth: inherit
}

headers {
  Accept: application/rdf+xml
  ~Accept: text/turtle
  ~Accept: application/n-triples
  ~Accept: application/ld+json
}

settings {
  encodeUrl: true
  timeout: 0
}
