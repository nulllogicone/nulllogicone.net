@page "{guid?}"
@model IndexModel
@{
    ViewData["Title"] = "Stamm";
}

@if (Model.Entity == null)
{
    <h1>Stamm</h1>
    <p>
        Diese Seite stellt die Entität <strong>Stamm</strong> dar. Hier können Informationen zu einem bestimmten Stamm angezeigt werden.
    </p>
    <p>
        Beispiel-URIref:
        <br />
        <a href="/ui/Stamm/b4111e0e-48d9-42c4-a6f6-ec4991264947">https://nulllogicone.net/ui/Stamm/b4111e0e-48d9-42c4-a6f6-ec4991264947</a>
    </p>
}
else
{
    <h1>Stamm @Model.Entity.Stamm1</h1>
    <p>
        Details zum Stamm mit GUID <strong>@Model.Entity.StammGuid</strong>:
    </p>
    <div>Name: @Model.Entity.Stamm1<br />

        Beschreibung: @Model.Entity.Beschreibung<br />
        image: @Model.Entity.Datei<br />
    </div>

}



